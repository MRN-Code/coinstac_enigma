{
  "meta": {
    "name": "ENIGMA in COINSTAC demo",
    "id": "enigma-py",
    "version": "v1.0.0",
    "repository": "",
    "description": "a test for decentralized PCA"
  },
  "computation": {
    "type": "docker",
    "dockerImage": "enigma-py",
    "command": [
      "python",
      "\/computation\/local.py"
    ],
    "remote": {
      "type": "docker",
      "dockerImage": "enigma",
      "command": [
        "python",
        "\/computation\/remote.py"
      ]
    },
    "input": {
      "samples": {
        "type": "array"
      }
    },
    "output": {
      "PC_global": {
        "type": "array"
      },
      "projM_global": {
        "type" : "array"      
      },
      "bkprojM_global" : {
        "type" : "array"
      }
    }
  }
}